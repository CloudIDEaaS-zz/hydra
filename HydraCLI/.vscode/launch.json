    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Hydra Installer - install",
            "preLaunchTask": "build",
            "cwd": "${workspaceFolder}/GenOutput/sample",
            "program": "${workspaceFolder}/HydraInstall/out/postinstall.js",
            "console": "externalTerminal",
            "args": []
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Hydra Cli - generate app",
            "preLaunchTask": "build",
            "cwd": "${workspaceFolder}/GenOutput/sample",
            "program": "${workspaceFolder}/HydraCli/out/cli.js",
            "console": "externalTerminal",
            "args": [
                "generate",
                "app",
                "--logToConsole",
                "--debug"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Hydra Cli - generate workspace",
            "preLaunchTask": "build",
            "cwd": "${workspaceFolder}/GenOutput/sample",
            "program": "${workspaceFolder}/HydraCli/out/cli.js",
            "console": "externalTerminal",
            "args": [
                "generate",
                "workspace",
                "--logToConsole",
                "--debug",
                "--appName",
                "contoso"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Hydra Cli - generate businessmodel",
            "preLaunchTask": "build",
            "cwd": "${workspaceFolder}/GenOutput/sample",
            "program": "${workspaceFolder}/HydraCli/out/cli.js",
            "console": "externalTerminal",
            "args": [
                "generate",
                "businessmodel",
                "--logToConsole",
                "--debug",
                "--template",
                "\"%HYDRASOLUTIONPATH%\\ApplicationGenerator\\GeneratorTemplates\\Default\\businessModel.template\""
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Hydra Cli - test",
            "preLaunchTask": "build",
            "cwd": "${workspaceFolder}/GenOutput/sample",
