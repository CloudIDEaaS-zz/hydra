function initMasks() {

	$(document).ready(function() {

		$('.phoneMask').mask("000-000-0000");
		$('.ssnMask').mask("000-00-0000");
		$('.moneyMask').mask('000.000.000.000.000,00', {
			reverse : true
		});
		$('.dateMask').mask('00/00/0000');
		$('.zipCodeMask').mask('00000');
		$('.accountNumberMask').mask('000000000000000000000');
		$('.routingNumberMask').mask('000000000000000');
		$('.creditCardNumberMask').mask('0000000000000000');
		$('.ccNumberHistoryMask').mask('0000');
		$('.achNumberHistoryMask').mask('0000');

	});

}