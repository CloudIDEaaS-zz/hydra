{
	"name": "@cloudideaas/hydra",
	"displayName": "Hydra",
	"description": "Hydra is a app generation tool with resulting source code. Generated front-end support for Ionic/Angular. Restful service layer support for .NET Core. Back-end support for SQL Server. Other supported technologies coming soon as Hydra is highly extensible. Also includes support for mobile capability, Docker, Helm, Azure Devops, and Kubernetes.",
	"version": "1.0.37",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CloudIDEaaS/hydra.git"
	},
	"keywords": [
		"codegen",
		"app generation",
		"code generation",
		"code scaffolding",
		".net",
		".net core",
		"sql server",
		"mobile app",
		"docker",
		"helm",
		"azure devops",
		"kubernetes"
	],
	"author": "CloudIDEaaS Inc",
	"license": "ISSC",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/",
		"access": "public"
	},
	"bugs": {
		"url": "https://github.com/CloudIDEaaS/hydra/issues"
	},
	"homepage": "https://github.com/CloudIDEaaS/hydra#readme",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.HydraCli"
	],
	"main": "./out/cli.js",
	"bin": {
		"hydra": "out/cli.js"
	},
	"contributes": {
		"commands": [
			{
				"command": "extension.HydraCli",
				"title": "HydraCli"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"preinstall": "npm install @cloudideaas/hydrainstall"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"grunt": "^1.3.0",
		"grunt-bump": "^0.8.0",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"bluebird": "^3.5.3",
		"cli-progress": "^3.0.0",
		"colors": "^1.3.2",
		"command-line-args": "^5.0.2",
		"js-beautify": "^1.8.8",
		"js-yaml": "^3.13.1",
		"linq-javascript": "^1.0.249",
		"node-cmd": "^3.0.0",
		"node-gyp": "^7.1.0",
		"path": "^0.12.7",
		"ps-node": "^0.1.6",
		"read-package-json": "^2.0.13",
		"readline": "^1.3.0",
		"rimraf": "^2.6.2",
		"shelljs": "^0.8.2",
		"stringbuilder": "0.0.11",
		"windows-environment": "^1.0.2",
		"windows-registry": "^0.1.5",
		"write-file": "^1.0.0",
		"yarn": "^1.12.3"
	}
}
