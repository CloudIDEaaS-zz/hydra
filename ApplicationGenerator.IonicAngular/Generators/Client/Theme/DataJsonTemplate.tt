<#@ template debug="false" hostspecific="false" language="C#" inherits="AbstraX.Generators.Base.TemplateBase"  #>
<#@ assembly name="System.Core" #>
<#@ import namespace="System.Linq" #>
<#@ import namespace="System.Text" #>
<#@ import namespace="System.Collections.Generic" #>
<#@ import namespace="Utils" #>
<#@ import namespace="AbstraX" #>
<#@ import namespace="AbstraX.Generators" #>
<#@ import namespace="AbstraX.Angular" #>
<#@ import namespace="AbstraX.Resources" #>
<#@ output extension=".html" #>
<#@ parameter type="System.EventHandler" name="DebugCallback" #>
<#@ parameter type="AbstraX.IResourceData" name="ResourceData" #>
<#@ parameter type="AbstraX.ObjectProperties.MarketingObjectProperties" name="MarketingObjectProperties" #>
{
  "welcome":
  [
    {
      "page":
      {
        "pageTitle": "<#= this.ResourceData.AppName.NullToEmpty() #>",
        "metaDescription": "",
        "metaKeywords": ""
      },
      "details":
      {
        "welcomeTitle": "Welcome to <#= this.ResourceData.AppName.NullToEmpty() #>",
        "description": "<#= this.ResourceData.AppDescription.NullToEmpty() #>",
        "startAppUrlTitle": "Skip this intro and start utilizing the <#= this.ResourceData.AppName.NullToEmpty() #> right away",
        "splashScreenAlt": "Splash screen for  <#= this.ResourceData.AppName.NullToEmpty() #>"
      }
    }
  ],
  "about":
  [
    {
      "page":
      {
        "pageTitle": "About the <#= this.ResourceData.AppName.NullToEmpty() #>",
        "metaDescription": "",
        "metaKeywords": ""
      },
      "details":
      {
        "header":
        {
          "aboutBannerAlt": "Banner for the <#= this.ResourceData.AppName.NullToEmpty() #>",
          "description": "<#= this.ResourceData.AppDescription.NullToEmpty() #>",
          "version": "$version"
        },
<#
    if (this.MarketingObjectProperties != null)
    {
        var other = this.MarketingObjectProperties.Other;
        var rateUs = this.MarketingObjectProperties.RateUs;
        var otherLinks = this.MarketingObjectProperties.OtherLinks;
        SocialMediaEntry[] socialMedia = null;
        AppStoreEntry[] appStores = null;

        if (this.MarketingObjectProperties.SocialMedia != null)
        {
            socialMedia = this.MarketingObjectProperties.SocialMedia.SocialMedia;
        }

        if (this.MarketingObjectProperties.RateUs != null)
        {
            appStores = this.MarketingObjectProperties.RateUs.AppStores;
        }

#>
        "mainPageLinks":
        [
<#
        if (other != null)
        {
            if (other.AdvertisingLink != null)
            {
                var linkProperties = this.MarketingObjectProperties.GetLinkProperties("AdvertisingLink", other.AdvertisingLink);
#>              
            {
                "title": "<#= linkProperties.LinkCallToAction #>",
                "text": "<#= linkProperties.LinkCallToAction #>",
                "url": "<#= other.AdvertisingLink #>"
            },
<#
            }

            if (other.ConnectWithUsLink != null)
            {
                var linkProperties = this.MarketingObjectProperties.GetLinkProperties("ConnectWithUsLink", other.ConnectWithUsLink);
#>
            {
                "title": "<#= linkProperties.LinkCallToAction #>",
                "text": "<#= linkProperties.LinkCallToAction #>",
                "url": "<#= other.ConnectWithUsLink #>"
            }
<#
            }

        }
#>
        ],
        "socialLinks":
        [
<#
        if (other != null)
        {
            if (other.EmailUsLink != null)
            {
                var linkProperties = this.MarketingObjectProperties.GetLinkProperties("EmailUsLink", other.EmailUsLink);
#>              
            {
                "visitTitle": "<#= linkProperties.LinkCallToAction #>",
                "visitText": "<#= linkProperties.LinkCallToAction #>",
                "tinyLogo": "data:image/gif;base64,",
                "smallLogo": "data:image/gif;base64,",
                "largeLogo": "data:image/gif;base64,",
                "logoAlt": "Email us",
                "accountUrl": "<#= other.EmailUsLink #>",
                "shareUrl": "<#= other.EmailUsLink #>"
            },
<#
            }
        }

        if (socialMedia != null)
        {
            var socialMediaState = socialMedia.WithState();

            foreach (var entry in socialMediaState)
            {
#>
            {
                "visitTitle": "<#= entry.VisitCallToAction #>",
                "visitText": "<#= entry.VisitCallToAction #>",
                "shareTitle": "<#= entry.ShareCallToAction #>",
                "shareText": "<#= entry.ShareCallToAction #>",
                "tinyLogo": "<#= entry.TinyLogo #>",
                "smallLogo": "<#= entry.SmallLogo #>",
                "largeLogo": "<#= entry.LargeLogo #>",
                "logoAlt": "<#= entry.Name #>",
                "accountUrl": "<#= entry.AccountUrl #>",
                "shareUrl": "<#= entry.ShareUrl #>",
                "enable": "<#= entry.Enable.ToString().LowerCase() #>"
            }<#= socialMediaState.IsLast ? string.Empty : "," #>
<#
            }                                             
        }

#>
        ],
        "appStoreLinks":
        [
<#
        if (appStores != null)
        {
            var appStoresState = appStores.WithState();

            foreach (var appStore in appStoresState)
            {
#>
            {
                "name": "<#= appStore.Name #>",
                "text": "<#= appStore.CallToAction #>",
                "title": "<#= appStore.CallToAction #>",
                "tinyLogo": "<#= appStore.TinyLogo #>",
                "smallLogo": "<#= appStore.SmallLogo #>",
                "largeLogo": "<#= appStore.LargeLogo #>",
                "logoAlt": "<#= appStore.Name #>",
                "accountUrl": "<#= appStore.AccountUrl #>",
                "device": "<#= appStore.DeviceRatingsOs #>",
                "allowRating": "<#= appStore.AllowRating.ToString().LowerCase() #>"
            }<#= appStoresState.IsLast ? string.Empty : "," #>
<#
            }                                             
        }
#>
        ],
        "tellOthers":
        [
<#
        if (other != null)
        {
            if (other.TellOthers != null)
            {
                var linkProperties = this.MarketingObjectProperties.GetLinkProperties("TellOthers", other.TellOthers.Url);
#>
            {
                "visitTitle": "<#= linkProperties.LinkCallToAction #>",
                "visitText": "<#= linkProperties.LinkCallToAction #>",
                "smallLogo": "data:image/gif;base64,",
                "logoAlt": "Tell others",
                "accountUrl": "<#= other.TellOthers.Url #>"
            }
<#
            }
        }
#>
        ],
        "sidelLinks":
        [
<#
        if (otherLinks != null)
        {
            var otherLinksState = otherLinks.WithState();

            foreach (var link in otherLinksState)
            {
                var linkProperties = this.MarketingObjectProperties.GetLinkProperties("Other", link);
#>
            {
                "title": "<#= linkProperties.LinkCallToAction #>",
                "text": "<#= linkProperties.LinkCallToAction #>",
                "url": "<#= link #>"
            }<#= otherLinksState.IsLast ? string.Empty : "," #>
<#
            }
        }
#>
        ]
<#
    }
    else
    {
#>
        "mainPageLinks":
        [

        ],
        "socialLinks":
        [
        ],
        "sidelLinks":
        [
        ]
<#
    }
#>
      }
    }
  ],
  "license":
  [
    {
      "page":
      {
        "pageTitle": "Licensing information for the <#= this.ResourceData.AppName.NullToEmpty() #>",
        "metaDescription": "",
        "metaKeywords": ""
      },
      "details":
      {
        "content": "<#= this.ResourceData.GetHtmlContent("EndUserLicense") #>",
        "licenseUrl": "<#= this.ResourceData.EndUserLicenseUrl.NullToEmpty() #>",
        "licenseUrlTitle": "This is the EULA (end user license agreement) for using of <#= this.ResourceData.AppName.NullToEmpty() #>",
        "questionsEmailAddress": "<#= this.ResourceData.EndUserLicenseEmailAddress.NullToEmpty() #>",
        "questionsEmailAddressTitle": "You can submit email questions regarding the usage of <#= this.ResourceData.AppName.NullToEmpty() #> by clicking this link",
        "questionsMailingAddress": "<#= this.ResourceData.EndUserLicenseMailingAddress.NullToEmpty() #>"
      }
    }
  ],
  "support":
  [
    {
      "page":
      {
        "pageTitle": "Support information for the <#= this.ResourceData.AppName.NullToEmpty() #>",
        "metaDescription": "",
        "metaKeywords": ""
      },
      "details":
      {
        "content": "<#= this.ResourceData.GetHtmlContent("SupportDetails") #>",
        "supportUrl": "<#= this.ResourceData.SupportUrl.NullToEmpty() #>",
        "supportUrlTitle": "This is the link for support details for usage of <#= this.ResourceData.AppName.NullToEmpty() #>",
        "supportEmailAddress": "<#= this.ResourceData.SupportEmailAddress.NullToEmpty() #>",
        "supportEmailAddressTitle": "You can submit email questions to retrieve support for the <#= this.ResourceData.AppName.NullToEmpty() #> by clicking this link"
      }
    }
  ],
  "pages":
  [
  ]
}